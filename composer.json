{
    "name": "suarez/laravel-utm-parameter",
    "description": "A little helper to store and handle utm-parameter",
    "homepage": "https://github.com/toni-suarez/laravel-utm-parameter",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "utm-parameter",
        "social-media",
        "utm"
    ],
    "authors": [
        {
            "name": "Toni Suarez",
            "email": "hallo@tonisuarez.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/contracts": "^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Suarez\\UtmParameter\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Suarez\\UtmParameter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "lint": "./vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Suarez\\UtmParameter\\Providers\\UtmParameterServiceProvider"
            ],
            "aliases": {
                "UtmParameter": "Suarez\\UtmParameter\\Facades\\UtmParameter"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
